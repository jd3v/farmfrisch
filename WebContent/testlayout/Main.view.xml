<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" id="mainer"
	xmlns:l="sap.ui.layout" xmlns:m="sap.m" xmlns="sap.suite.ui.commons"
	controllerName="testApp.testlayout.Main" xmlns:html="http://www.w3.org/1999/xhtml">


	<m:Page title="Main" enableScrolling="false" showHeader="false">
		<m:content>
			<m:TileContainer id="container" tileDelete="handleTileDelete">

				<m:CustomTile>
					<!-- press="navHowItWorks" -->
					<GenericTile size="M" frameType="OneByOne"
						backgroundImage="img/farmfrisch_logo_184x176.jpg" type="Navigation">
					</GenericTile>
				</m:CustomTile>

			
								<m:CustomTile class="farmFrischTile">
					<GenericTile size="M" frameType="OneByOne" press="navProductOV"
						backgroundImage="img/offer.png" info="{prodgroupID}" type="Navigation">
						<tileContent>
							<TileContent size="M">
								<content>
									<NewsContent size="M" subheader="Angebot des Tages!">
									</NewsContent>
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</m:CustomTile>


					<m:CustomTile class="farmFrischTile">
						<GenericTile size="M" frameType="OneByOne" press="navProductOV"
							backgroundImage="img/products.png" info="{prodgroupID}" type="Navigation">
							<tileContent>
								<TileContent size="M">
									<content>
										<NewsContent size="M" subheader="Produkte in Ihrer Nähe">
										</NewsContent>
									</content>
								</TileContent>
							</tileContent>
						</GenericTile>
					</m:CustomTile>

					<m:CustomTile>

						<DynamicContainer displayTime="4250" transitionTime="500">

							<tiles>

								<GenericTile size="M" class="farmFrischTile"
									frameType="OneByOne" press="navProductOV" backgroundImage="img/products/wine.png"
									info="{id}" type="Navigation">
									<tileContent>
										<TileContent size="M">
											<content>
												<l:VerticalLayout class="marginVerticalContent">
													<m:RatingIndicator maxValue="5" value="4.5" />
												</l:VerticalLayout>
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>

								<GenericTile size="M" class="farmFrischTile"
									frameType="OneByOne" press="navProductOV" backgroundImage="img/products/salad.png"
									info="{id}" type="Navigation">
									<tileContent>
										<TileContent size="M">
											<content>
												<l:VerticalLayout class="marginVerticalContent">
													<m:RatingIndicator maxValue="5" value="5" />
												</l:VerticalLayout>

											</content>
										</TileContent>
									</tileContent>
								</GenericTile>


								<GenericTile size="M" class="farmFrischTile"
									frameType="OneByOne" press="navProductOV" backgroundImage="img/products/radieschen.png"
									info="{id}" type="Navigation">
									<tileContent>
										<TileContent size="M">
											<content>
												<l:VerticalLayout class="marginVerticalContent">
													<m:RatingIndicator maxValue="5" value="4" />
												</l:VerticalLayout>
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>

								<GenericTile size="M" class="farmFrischTile"
									frameType="OneByOne" press="navProductOV" backgroundImage="img/products/beef.png"
									info="{id}" type="Navigation">
									<tileContent>
										<TileContent size="M">
											<content>
												<l:VerticalLayout class="marginVerticalContent">
													<m:RatingIndicator maxValue="5" value="4" />
												</l:VerticalLayout>
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>


							</tiles>
						</DynamicContainer>

					</m:CustomTile>


					<m:CustomTile class="farmFrischTile">

						<GenericTile size="M" class="farmFrischTile"
							frameType="OneByOne" press="navFarmerOV" backgroundImage="img/Farmer.png"
							info="{id}" type="Navigation">
							<tileContent>
								<TileContent size="M">
									<content>
										<NewsContent size="M" subheader="Farmer in Ihrer Nähe">
										</NewsContent>
									</content>
								</TileContent>
							</tileContent>
						</GenericTile>
					</m:CustomTile>

					<m:CustomTile>

						<DynamicContainer displayTime="2750" transitionTime="500">

							<tiles>

								<GenericTile size="M" class="farmFrischTile"
									frameType="OneByOne" press="navFarmerOV" backgroundImage="img/farmers/farmer1.png"
									info="{id}" type="Navigation">
									<tileContent>
										<TileContent size="M">
											<content>
												<l:VerticalLayout class="marginVerticalContent">
													<m:RatingIndicator maxValue="5" value="4" />
												</l:VerticalLayout>

											</content>
										</TileContent>
									</tileContent>
								</GenericTile>


								<GenericTile size="M" class="farmFrischTile"
									frameType="OneByOne" press="navFarmerOV" backgroundImage="img/farmers/farmer3.png"
									info="{id}" type="Navigation">
									<tileContent>
										<TileContent size="M">
											<content>
												<l:VerticalLayout class="marginVerticalContent">
													<m:RatingIndicator maxValue="5" value="4.5" />
												</l:VerticalLayout>
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>

								<GenericTile size="M" class="farmFrischTile"
									frameType="OneByOne" press="navFarmerOV" backgroundImage="img/farmers/farmer4.png"
									info="{id}" type="Navigation">
									<tileContent>
										<TileContent size="M">
											<content>
												<l:VerticalLayout class="marginVerticalContent">
													<m:RatingIndicator maxValue="5" value="5" />
												</l:VerticalLayout>
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>

								<GenericTile size="M" class="farmFrischTile"
									frameType="OneByOne" press="navFarmerOV" backgroundImage="img/farmers/farmer2.png"
									info="{id}" type="Navigation">
									<tileContent>
										<TileContent size="M">
											<content>
												<l:VerticalLayout class="marginVerticalContent">
													<m:RatingIndicator maxValue="5" value="3.5" />
												</l:VerticalLayout>
											</content>
										</TileContent>
									</tileContent>
								</GenericTile>


							</tiles>
						</DynamicContainer>

					</m:CustomTile>


					<m:CustomTile class="farmFrischTile">
						<GenericTile size="M" class="farmFrischTile"
							frameType="OneByOne"  backgroundImage="img/event.png"
							info="{id}" type="Navigation">
							<tileContent>
								<TileContent size="M">
									<content>
										<NewsContent size="M" subheader="Events in Ihrer Nähe">
										</NewsContent>
									</content>
								</TileContent>
							</tileContent>
						</GenericTile>
					</m:CustomTile>


			</m:TileContainer>


		</m:content>
		<m:footer>
			<core:Fragment fragmentName="testApp.testlayout.fragments.MainFooter" type="XML"/>
		</m:footer>
	</m:Page>

</mvc:View>