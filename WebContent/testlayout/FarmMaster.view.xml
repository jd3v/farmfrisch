<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="testApp.testlayout.FarmMaster" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Master Farm Profile"  showNavButton="true" navButtonPress="back">
		<content>
			<List			
			    headerText="Farmers Product(s)"
			    items="{
			      path: 'ProducerLocationsGrouped'
			      }">
			    <StandardListItem
			      title="{locName}"
			      type="Navigation"
			      press="markLoc"/>
			         
			</List>			
		</content>
		<footer>
			<core:Fragment fragmentName="testApp.testlayout.fragments.MasterFooter" type="XML"/>
		</footer>
	</Page>
</core:View>