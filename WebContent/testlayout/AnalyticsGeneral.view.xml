<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:m="sap.m" xmlns="sap.suite.ui.commons" controllerName="testApp.testlayout.ProductOV"
	xmlns:html="http://www.w3.org/1999/xhtml">


	<m:Page title="General Analytics" enableScrolling="false"
		showHeader="false" class="analytics">
		<m:content>


			<m:Panel text="Account Overview Information">
				<m:content>
					<KpiTile value="Premium Account" description="Account Status"
						doubleFontSize="false" valueStatus="Good" />
					<KpiTile value="+15.000" description="Aufrufe insgesamt"
						valueStatus="Good" />
					<KpiTile value="126" description="Großaufträge erhalten"
						valueStatus="Good" />
					<KpiTile value="18" description="Produkte im Portfolio" />
				</m:content>
			</m:Panel>

			<m:TileContainer id="container" tileDelete="handleTileDelete">

				<m:CustomTile>
					<GenericTile header="Seitenaufrufe im Monat November"
						size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="Aufrufe" size="M">
								<content>
									<NumericContent size="M" value="623" valueColor="Good"
										indicator="Up" icon="sap-icon://group" />
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</m:CustomTile>

				<m:CustomTile>
					<GenericTile header="Unique Seitenaufrufe im Monat November"
						size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="Aufrufe" size="M">
								<content>
									<NumericContent size="M" value="284"
										icon="sap-icon://hr-approval" />
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</m:CustomTile>

				<m:CustomTile>
					<GenericTile header="Durchschnittliche Zeit eines Profilbesuchs"
						size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="Sekunden" size="M">
								<content>
									<NumericContent size="M" value="57" valueColor="Good"
										indicator="Up" icon="sap-icon://time-entry-request" />
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</m:CustomTile>

				<m:CustomTile>
					<GenericTile header="Bounce-Rate" subheader="Anteil besonders kurzer Aufrufe"
						size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="aller Aufrufe" size="M">
								<content>
									<NumericContent size="M" value="34" valueColor="Critical"
										scale="%" indicator="Down" icon="sap-icon://media-forward" />
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</m:CustomTile>

				<m:CustomTile>
				
					<GenericTile header="Comparative Annual Totals"
						subheader="By Region" size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="Compare across regions" unit="EUR"
								size="M">
								<content>
									<MicroAreaChart width="164px" height="74px"
										minXValue="0" maxXValue="100" minYValue="0" maxYValue="100"
										class="marginTopLeft" press="press">
										<firstXLabel>
											<MicroAreaChartLabel label="June 1"
												color="Good" />
										</firstXLabel>
										<lastXLabel>
											<MicroAreaChartLabel label="June 30"
												color="Critical" />
										</lastXLabel>
										<firstYLabel>
											<MicroAreaChartLabel label="0M" color="Good" />
										</firstYLabel>
										<lastYLabel>
											<MicroAreaChartLabel label="80M" color="Critical" />
										</lastYLabel>
										<chart>
											<MicroAreaChartItem>
												<points>
													<MicroAreaChartPoint x="0" y="0" />
													<MicroAreaChartPoint x="30" y="20" />
													<MicroAreaChartPoint x="60" y="20" />
													<MicroAreaChartPoint x="100" y="80" />
												</points>
											</MicroAreaChartItem>
										</chart>
										<target>
											<MicroAreaChartItem>
												<points>
													<MicroAreaChartPoint x="0" y="0" />
													<MicroAreaChartPoint x="30" y="30" />
													<MicroAreaChartPoint x="60" y="40" />
													<MicroAreaChartPoint x="100" y="90" />
												</points>
											</MicroAreaChartItem>
										</target>
										<maxThreshold>
											<MicroAreaChartItem color="Good">
												<points>
													<MicroAreaChartPoint x="0" y="0" />
													<MicroAreaChartPoint x="30" y="40" />
													<MicroAreaChartPoint x="60" y="50" />
													<MicroAreaChartPoint x="100" y="100" />
												</points>
											</MicroAreaChartItem>
										</maxThreshold>
										<minThreshold>
											<MicroAreaChartItem color="Error">
												<points>
													<MicroAreaChartPoint x="0" y="0" />
													<MicroAreaChartPoint x="30" y="20" />
													<MicroAreaChartPoint x="60" y="30" />
													<MicroAreaChartPoint x="100" y="70" />
												</points>
											</MicroAreaChartItem>
										</minThreshold>
									</MicroAreaChart>

								</content>
							</TileContent>
						</tileContent>
					</GenericTile>

				</m:CustomTile>

				<m:CustomTile>

					<GenericTile header="123 Demo Opportunity" subheader="SAP Jam Group"
						size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="Hours since last Activity" size="M">
								<content>
									<!-- <BulletChart size="M" scale="M" targetValue="100" forecastValue="50" 
										class="marginTopLeft" press="press"> -->
									<!-- <actual> -->
									<!-- <BulletChartData value="50" color="Good"/> -->
									<!-- </actual> -->
									<!-- <thresholds> -->
									<!-- <BulletChartData value="0" color="Error"/> -->
									<!-- <BulletChartData value="50" color="Critical"/> -->
									<!-- <BulletChartData value="150" color="Critical"/> -->
									<!-- <BulletChartData value="160" color="Error"/> -->
									<!-- </thresholds> -->
									<!-- </BulletChart> -->

									<BulletChart size="M" scale="M" targetValue="100"
										forecastValue="110" class="marginTopLeft" press="press">
										<actual>
											<BulletChartData value="120" color="Good" />
										</actual>
										<thresholds>
											<BulletChartData value="0" color="Error" />
											<BulletChartData value="50" color="Critical" />
											<BulletChartData value="150" color="Critical" />
											<BulletChartData value="200" color="Error" />
										</thresholds>
									</BulletChart>


								</content>

							</TileContent>
						</tileContent>
					</GenericTile>
				</m:CustomTile>

			</m:TileContainer>

		</m:content>
		<m:footer>
			<m:Toolbar>
				<m:ToolbarSpacer />
				<m:Button text="Share" icon="./img/fb_white_150x150.png" />
				<m:Button text="Tweet" icon="./img/twitter_white_150x150.png" />
				<m:Button icon="sap-icon://settings" />
			</m:Toolbar>
		</m:footer>
	</m:Page>

</core:View>