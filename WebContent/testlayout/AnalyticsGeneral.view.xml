<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns:m="sap.m" xmlns="sap.suite.ui.commons" controllerName="testApp.testlayout.ProductOV"
	xmlns:html="http://www.w3.org/1999/xhtml">


	<m:Page title="General Analytics" enableScrolling="false"
		showHeader="false" class="analytics">
		<m:content>


			<m:Panel text="Account Overview Information">
				<m:content>
					<KpiTile value="Premium Account" description="Account Status"
						doubleFontSize="false" valueStatus="Good" />
					<KpiTile value="+15.000" description="Aufrufe insgesamt"
						valueStatus="Good" />
					<KpiTile value="126" description="Großaufträge erhalten"
						valueStatus="Good" />
					<KpiTile value="18" description="Produkte im Portfolio" />
				</m:content>
			</m:Panel>

			<m:TileContainer id="container" tileDelete="handleTileDelete">

				<m:CustomTile>
					<GenericTile header="Seitenaufrufe im Monat November"
						size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="Aufrufe" size="M">
								<content>
									<NumericContent size="M" value="623" valueColor="Good"
										indicator="Up" icon="sap-icon://group" />
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</m:CustomTile>

				<m:CustomTile>
					<GenericTile header="Unique Seitenaufrufe im Monat November"
						size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="Aufrufe" size="M">
								<content>
									<NumericContent size="M" value="284" icon="sap-icon://hr-approval" />
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</m:CustomTile>

				<m:CustomTile>
					<GenericTile header="Seitenaufrufe im Monat November"
						size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="Aufrufe" size="M">
								<content>
									<NumericContent size="M" value="3" valueColor="Good"
										indicator="Up" icon="sap-icon://employee-lookup" />
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</m:CustomTile>

				<m:CustomTile >
					<GenericTile header="Burst-Rate" subheader="Anteil besonders kurzer Aufrufe"
						size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="aller Aufrufe" size="M">
								<content>
									<NumericContent size="M" value="34" valueColor="Critical"
										scale="%" indicator="Down" icon="sap-icon://media-forward" />
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>
				</m:CustomTile>

				<m:CustomTile>

				</m:CustomTile>

































				<m:CustomTile>





					<GenericTile header="Comparative Annual Totals"
						subheader="By Region" size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="Compare across regions" unit="EUR"
								size="M">
								<content>
									<ComparisonChart size="M" scale="M">
										<data>
											<ComparisonData title="Americas" value="234"
												color="Good" />
											<ComparisonData title="EMEA" value="97" color="Error" />
											<ComparisonData title="APAC" value="197"
												color="Critical" />
										</data>
									</ComparisonChart>
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>

				</m:CustomTile>

				<m:CustomTile>

					<GenericTile header="123 Demo Opportunity" subheader="SAP Jam Group"
						size="M" frameType="OneByOne" press="press">
						<tileContent>
							<TileContent footer="Hours since last Activity" size="M">
								<content>
									<NumericContent size="M" value="9"
										icon="sap-icon://locked" />
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>


				</m:CustomTile>

				<m:CustomTile>

					<GenericTile header="Comparative Annual Totals"
						subheader="Expenses By Region" size="M" frameType="TwoByOne"
						press="press">
						<tileContent>
							<TileContent footer="Actual and Target" unit="EUR"
								size="M">
								<content>
									<NumericContent size="M" scale="M" value="1.96"
										valueColor="Error" indicator="Up" />
								</content>
							</TileContent>
							<TileContent footer="Compare across regions" unit="EUR"
								size="M">
								<content>
									<ComparisonChart size="M" scale="M">
										<data>
											<ComparisonData title="Americas" value="234"
												color="Good" />
											<ComparisonData title="EMEA" value="97" color="Error" />
											<ComparisonData title="APAC" value="197"
												color="Critical" />
										</data>
									</ComparisonChart>
								</content>
							</TileContent>
						</tileContent>
					</GenericTile>

				</m:CustomTile>

			</m:TileContainer>

		</m:content>
		<m:footer>
			<m:Toolbar>
				<m:Button text="Zurück" icon="sap-icon://nav-back" press="backToLandingPage" />
				<m:ToolbarSpacer />
				<m:Button text="Share" icon="./img/fb_white_150x150.png" />
				<m:Button text="Tweet" icon="./img/twitter_white_150x150.png" />
				<m:Button icon="sap-icon://settings" />
			</m:Toolbar>
		</m:footer>
	</m:Page>

</core:View>