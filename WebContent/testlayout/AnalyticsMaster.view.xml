<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" 
		controllerName="testApp.testlayout.AnalyticsMaster" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Analytics">
		<content>
			 <List
			    id="ShortProductList"
			    items="{
			      path: '/ListItems'			      
			    }"
			    mode="SingleSelectMaster"
			    select="navigateToDetail">
			    <items>
			      <StandardListItem
			        title="{listName}"
			        type="Navigation"
			         />
			    </items>
			  </List>
		</content>
		<footer>
			<core:Fragment fragmentName="testApp.testlayout.fragments.MasterFooter" type="XML"/>
		</footer>
	</Page>
</core:View>